<!ELEMENT related.term  ( %type.String; ) >
<!ATTLIST related.term  %ref.TermDef;  #REQUIRED >

<!ELEMENT Term  %type.LimitedNarrative; >
<!ATTLIST Term  %ref.TermDef;  #REQUIRED >

<!ELEMENT ValueType       ( %common.elements; ) >
<!ELEMENT NoValueReason   ( %common.elements; ) >
<!ELEMENT FileFormat      ( %common.elements; ) >
<!ELEMENT ContentType     ( %common.elements; ) >

  <!-- ================ END OF EC DTD   ================ -->

