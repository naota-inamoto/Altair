;;;
;;;	助動詞活用型 および 活用形
;;;

(無活用型
    ((語幹      *   )
     (基本形    *   )
    )
)

(助動詞ぬ型
    ((語幹             *   )
     (基本形           ぬ  )
     (基本条件形       ねば)
     (基本連用形       ず  )
     (基本推量形       ぬだろう)
     (タ形             なんだ)
     (タ系条件形       なんだら)
     (タ系連用テ形     ないで)
     (タ系推量形       なんだろう)

     (音便基本形       ん  )	; 基本形
     (音便推量形       んだろう); 基本推量形

     (文語連体形       ざる)	; 基本連体形
     (文語条件形       ざれば)	; 基本条件形
     (文語音便条件形   ずんば)  ; 基本条件形
    )
) 

(助動詞だろう型
    ((語幹                *       )
     (基本形              だろう  )
     (ダ列基本条件形      ならば  )
     (デアル列基本推量形  であろう)
     (デス列基本推量形    でしょう)
     (デス列省略推量形    でしょ  )
    )
)

(助動詞そうだ型
    ((語幹             *         )
     (基本形           そうだ    )
     (ダ列タ系連用テ形 そうで    )
     (デアル列基本形   そうである)
     (デス列基本形     そうです  )
    )
)

(助動詞く型				; 「べし」，「ごとし」
    ((語幹             *       )
     (基本形           し      )
     (基本連用形       く      )
     (文語連体形       き      )
    )
)

