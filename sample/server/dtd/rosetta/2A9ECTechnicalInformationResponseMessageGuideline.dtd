<!-- ================  Both QUERY and RESPONSE  ================ -->

<!ELEMENT Pip2A9ECTechnicalInformationResponse (
  dict.identifier,
  response,
  %MessageMetadata;,
  requestingDocumentIdentifier?,
  requestingDocumentDateTime?
  ) >
<!ATTLIST Pip2A9ECTechnicalInformationResponse
  qd.key  CDATA  #IMPLIED
  >
<!METAINFO Pip2A9ECTechnicalInformationResponse "data.duplicate" "false">

<!ELEMENT dict.identifier  (#PCDATA) >
<!ATTLIST dict.identifier  %xmlAtts; >

<!ELEMENT element  (
  name,
  value*
  ) >
<!ATTLIST element
  id  ID  #IMPLIED
  %dictionaryReference;
  >

<!ELEMENT name  (#PCDATA)  >
<!ATTLIST name  %xmlAtts; >

<!ELEMENT value  (#PCDATA)  >
<!ATTLIST value
  %xmlAtts;
  type           CDATA            #IMPLIED
  ntol.abs       %toleranceAbs;   #IMPLIED
  ntol.per       %tolerancePer;   #IMPLIED
  ptol.abs       %toleranceAbs;   #IMPLIED
  ptol.per       %tolerancePer;   #IMPLIED
  NoValueReason  CDATA            #IMPLIED
  >

<!ELEMENT CharacteristicSet  ( name ) >
<!ATTLIST CharacteristicSet
  id       ID      #IMPLIED
  %dictionaryReference;
  members  IDREFS  #IMPLIED
  >

<!-- ==================  RESPONSE  =================== -->

<!ELEMENT response  (
  status,
  part.info*,
  ec.dictionary.add?
  ) >
<!ATTLIST response
  starting.at  NMTOKEN  #IMPLIED
  returned     NMTOKEN  #IMPLIED
  matched      NMTOKEN  #IMPLIED
  >

<!ELEMENT status  (
    s.success
  | s.fail
  | s.badquery
  | s.nomatch
  | s.not.available
  | s.reschedule.query
  ) >

<!ELEMENT s.success        (#PCDATA)  >
<!ATTLIST s.success        %xmlAtts; >
                
<!ELEMENT s.fail           (#PCDATA)  >
<!ATTLIST s.fail           %xmlAtts; >
                
<!ELEMENT s.badquery       (#PCDATA)  >
<!ATTLIST s.badquery       %xmlAtts; >
                
<!ELEMENT s.nomatch        (#PCDATA)  >
<!ATTLIST s.nomatch        %xmlAtts; >
                
<!ELEMENT s.not.available  (#PCDATA)  >
<!ATTLIST s.not.available  %xmlAtts; >

<!ELEMENT s.reschedule.query  (#PCDATA) >

<!ELEMENT part.info  (
  element*,
  CharacteristicSet*,
  PIO*
  ) >
<!ATTLIST part.info
  id  CDATA  #IMPLIED
  >

<!ELEMENT ec.dictionary.add  (
  class*,
  CharacteristicDefinition*,
  ConditionDefinition*,
  PropertyDefinitionSet*,
  TermDefinition*,
  ContentType*,
  FileFormat*,
  value.codes*
  ) >

<!ELEMENT value.codes  ( dic.value+ ) >
<!ATTLIST value.codes  %dictionaryReference; >

<!-- ==================  PIO  =================== -->

<!ELEMENT PIO (
  Version?,
  EffectiveDate?,
  Location*,
  Reference*,
  ObjectName,
  Legal*,
  ObjectId,
  Supplier?,
  Description?,
  FileSize?,
  ModificationDate?,
  ContentTypeRef?,
  FileFormatRef?,
  TargetSoftware*
  ) >
<!ATTLIST PIO
  NoValueReason  CDATA  #IMPLIED
  href           CDATA  #REQUIRED
  >

<!ELEMENT Version (#PCDATA) >
<!ATTLIST Version  %xmlAtts; >
         
<!ELEMENT EffectiveDate     (#PCDATA) >
             
<!ELEMENT Location          (#PCDATA) >

<!ELEMENT Reference         (#PCDATA) >

<!ELEMENT ObjectName        (#PCDATA) >
<!ATTLIST ObjectName        %xmlAtts; >

<!ELEMENT Legal             (#PCDATA) >
<!ATTLIST Legal             %xmlAtts; >

<!ELEMENT ObjectId          (#PCDATA) >
<!ATTLIST ObjectId          %xmlAtts; >

<!ELEMENT Supplier          (#PCDATA) >

<!ELEMENT Description       (#PCDATA) >
<!ATTLIST Description       %xmlAtts; >

<!ELEMENT FileSize          (#PCDATA) >

<!ELEMENT ModificationDate  (#PCDATA) >

<!ELEMENT ContentTypeRef    (#PCDATA) >

<!ELEMENT FileFormatRef     (#PCDATA) >

<!ELEMENT TargetSoftware    (#PCDATA) >
<!ATTLIST TargetSoftware    %xmlAtts; >

