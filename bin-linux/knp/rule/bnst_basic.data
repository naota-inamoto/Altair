;;;;; 〜を〜に
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

( ( ?* ) ( ( (?* (* * * * * ((〜を〜に構成語))) 
		 (助詞 * * * (に)) ?*) ) ) ( ?* ) 〜を〜に可能 )


;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;;;; タグ単位間の係り受け
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

;; 「〜する/のは」
;; この表現に係るタグ単位は「の」ではなく「〜する」に係るとする
( ( ?* ) ( ( ( ?* (名詞 * * * (の)) ?* ) ) ) ( ?* ) タグ単位受:-1 )

;; 「ごぼうの洗い方」
;; ノ格の要素は「洗い」に係る
;; 他の連体修飾は「方」に係る
( ( ?* ) ( ( (?* ^(助詞 * * * (の)) ) ((連体修飾)) ) ) ( ?* ) タグ単位受無視 )
( ( ?* ) ( ( ( ?* (動詞) (接尾辞 * * (方)) ?* ) ) ) ( ?* ) タグ単位受:-1 )


